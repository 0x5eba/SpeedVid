{
  "name": "SpeedVid",
  "version": "0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "match_about_blank": true,
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "storage", "alarms"],
  "browser_action": {
    "default_title": "SpeedVid",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}